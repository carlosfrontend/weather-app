{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-alert": "off",
      "quotes": [
        "error",
        "single",
        { "avoidEscape": true, "allowTemplateLiterals": false }
      ]
    }
}
